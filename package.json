{
  "name": "webframe-docs",
  "version": "0.0.1",
  "description": "Automatic Documentation Generator for React Components",
  "homepage": "https://github.com/webframer/docs",
  "author": "https://github.com/ecoinomist",
  "repository": {
    "type": "git",
    "url": "https://github.com/webframer/docs.git"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "pnpm watch",
    "build": "pnpm compile",
    "parse": "node src/parsePropTypes.js",
    "compile": "node src/compileDocs.js",
    "watch": "pnpm compile --watch"
  },
  "keywords": [
    "docs",
    "documentation",
    "generator",
    "propTypes",
    "mdx",
    "automatic",
    "compiler",
    "react",
    "components",
    "webframe"
  ],
  "dependencies": {
    "@webframer/js": "workspace:*",
    "chalk": "^5.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "compiler": "workspace:*",
    "core": "workspace:*",
    "fast-glob": "^3.2.12"
  },
  "devDependencies": {
    "tailwindcss": "3.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": "./bin.js"
}
